{
  "$test": {
    "hello": true
  },
  "aws-account": {
    ">>": {
      "image-bucket": {
        "accelerate": true,
        "error": "error.html"
      }
    },
    "$test2": {
      "hello": false,
      "world": true
    },
    "east": {
      "image-bucket": "<< aws-bucket.image << $test >> $test2",
      "basic-bucket": "<= aws-bucket.image >> $test << $test2"
    }
  },
  "aws-bucket": {
    ">>": {
      "grant": "id=xxx"
    },
    "$test": {
      "index": "index.html"
    },
    "image": {
      "<<?staging": {
        "name": "company-images-stag"
      },
      "<<?production": {
        "name": "company-images-prod"
      },
      "<<?$test": {
        "test": true
      },
      "index2": "<= $test.index",
      "index3": "<= aws-bucket.$test.index",
      "hello": false
    }
  }
}
